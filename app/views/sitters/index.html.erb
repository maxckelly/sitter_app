<p id="notice"><%= notice %></p>

<h1> Hi, <%= @sitter.name %> </h1>
<p> Here are the jobs you have coming up </p>

<% @meetings.each do |meeting| %>
  <div class="card">
    <div class="card-header">Booking</div>
      <div class="card-body">
        <h5 class="card-title"><%= meeting.name %></h5>
        <p class="card-text"><%= meeting.start_time %> - <%= meeting.end_time %></p>
        <a href="#" class="btn btn-primary">Go somewhere</a>
      </div>
  </div>


  <% end %>


